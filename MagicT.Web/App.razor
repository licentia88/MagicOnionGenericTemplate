
<CascadingValue Name="@(nameof(LoginManager))" Value="@LoginManager">
    <CascadingValue Name="@(nameof(SignOutFunc))" Value="@SignOutFunc">
                    <Router AppAssembly="@typeof(App).Assembly" AdditionalAssemblies="@(new[] { typeof(MagicT.Web.Shared.Pages.Audits.AuditsQuery).Assembly })">
                        <Found Context="routeData">

                            @{
                                if (IsLoaded)
                                {
                                    <RouteView RouteData="@routeData" DefaultLayout="@typeof(MainLayout)" />
                                    <FocusOnNavigate RouteData="@routeData" Selector="h1" />
                                }
                            }

                        </Found>
                        <NotFound>
                            <PageTitle>Not found</PageTitle>
                            <LayoutView Layout="@typeof(MainLayout)">
                                <p role="alert">Sorry, there's nothing at this address.</p>
                            </LayoutView>
                        </NotFound>
                    </Router>
    </CascadingValue>
</CascadingValue>


 