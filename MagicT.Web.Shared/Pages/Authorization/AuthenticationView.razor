@{

    @ChildContent

    if (!IsAuthenticated)
    {
        if (NotAuthenticated is not null)
        {
            @NotAuthenticated;
            return;
        }
        else
        {
            NavigationManager.NavigateTo("/");
            return;
        }
           
    }

    if (IsAuthenticated && Authenticated is not null)
    {
        @Authenticated;
    }
  
}


