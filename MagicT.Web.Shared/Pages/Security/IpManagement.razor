@page "/IpManagement"

@inherits PageBaseClass

 
<GenGrid 
         DataSource="DataSource"
         TModel="ClientData"
         EditMode="EditMode.Inline"
         Load="OnLoad"
         Title="IP Management">
    <GenColumns>
        <GenTextField Label="IpAddress" BindingField="@(nameof(ClientData.Ip))"/>
        <GenTextField Label="UserAgent" BindingField="@(nameof(ClientData.Status))"/>
    </GenColumns>
    <GenSideButtons>
        <MudIconButton OnClick="x=> Lock(context)"   Icon="@MaterialDesignIcons.Normal.Lock"/>
        <MudIconButton OnClick="x=> UnLock(context)" Icon="@MaterialDesignIcons.Normal.LockOpen"></MudIconButton>
    </GenSideButtons>
</GenGrid>